{{#each documents}}
  <div class="panel panel-doc">
    <div class="panel-heading {{json.style}}">
      {{>doc-title-group}}
    </div>
    <div class="panel-body" id="doc-{{this.id}}-content">  <!-- add class collapse to have it start collapsed-->
      {{>doc-version-tabs}}
      <div class="doc-content-container" data-parent-id={{this.id}} data-parent-type="Document">
        {{>doc-content}}
      </div>
    </div>
  </div>
{{/each}}
